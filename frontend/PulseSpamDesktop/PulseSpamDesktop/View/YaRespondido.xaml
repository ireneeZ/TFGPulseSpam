<Window x:Class="PulseSpamDesktop.View.YaRespondido"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PulseSpamDesktop.View"
        mc:Ignorable="d"
        Title="Pregunta completada" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock FontWeight="Bold" FontSize="24">
            Ya has respondido a la pregunta del día</TextBlock>
        <TextBlock FontSize="14" HorizontalAlignment="Center">
            Puedes cerrar la aplicación</TextBlock>
    </StackPanel>
</Window>
